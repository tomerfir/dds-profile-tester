<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="https://community.rti.com/schema/7.3.0/rti_dds_qos_profiles.xsd"
     version="7.3.0">

  <types>
    <struct name="KeyValuePair">
      <member name="id" type="int64" key="true" />
      <member name="key" type="string" stringMaxLength="64" key="true" />
      <member name="value" type="string" stringMaxLength="2048" />
    </struct>
  </types>

  <domain_library name="SimpleDomainLibrary">
    <domain name="SimpleDomain" domain_id="10">
      <register_type name="KeyValuePairType" type_ref="KeyValuePair" />
      <topic name="EnvironmentTopic" register_type_ref="KeyValuePairType" />
    </domain>
  </domain_library>

  <domain_participant_library name="SimpleParticipantLibrary">
    <domain_participant name="ParticipantA" domain_ref="SimpleDomainLibrary::SimpleDomain">
      <publisher name="PublisherA">
        <data_writer name="WriterA" topic_ref="EnvironmentTopic"/>
      </publisher>
      <subscriber name="SubscriberA">
        <data_reader name="ReaderA" topic_ref="EnvironmentTopic" />
      </subscriber>
    </domain_participant>
    <domain_participant name="ParticipantB" domain_ref="SimpleDomainLibrary::SimpleDomain">
      <publisher name="PublisherB">
        <data_writer name="WriterB" topic_ref="EnvironmentTopic"/>
      </publisher>
      <subscriber name="SubscriberB">
        <data_reader name="ReaderB" topic_ref="EnvironmentTopic" />
      </subscriber>
    </domain_participant>
  </domain_participant_library>
</dds>